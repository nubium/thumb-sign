{
	"name":        "nubium/thumb-sign",
	"description": "Signing of thumbnail links",
	"keywords":    ["thumbnails", "security", "signing"],
	"license":     "MIT",
	"require": {
		"php": ">=7.4",
		"nette/http": "^3.1.5"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"mockery/mockery": "~1.5.0",
		"phpstan/phpstan": "^1.4.2",
		"phpstan/phpstan-mockery": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/extension-installer": "^1.1.0"
	},
	"autoload": {
		"psr-4": {
			"Nubium\\ThumbSign\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Nubium\\ThumbSign\\Test\\": "test"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
